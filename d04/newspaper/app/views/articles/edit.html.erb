<%= form_for @article, url: {action: 'update'} do |f| %>

<h2><%= f.label :headline %></h2>
<%= f.text_field :headline, value: @article.headline %>
<br>

<h2><%= f.label :byline %></h2>
<%= f.text_field :byline, byline: @article.byline %>
<br>

<h2><%= f.label :content %></h2>
<%= f.text_area :content, content: @article.content %>
<br>

<h2><%= f.label :photo_url %></h2>
<%= f.text_field :photo_url, byline: @article.byline %>
<br>

<%= f.submit "Edit" %>

<% end %>
