<%= form_for @article, url: {action: 'create'} do |f| %>

<h2><%= f.label :headline %></h2>
<%= f.text_field :headline %>
<br>

<h2><%= f.label :byline %></h2>
<%= f.text_field :byline %>
<br>

<h2><%= f.label :content %></h2>
<%= f.text_area :content %>
<br>

<h2><%= f.label :photo_url %></h2>
<%= f.text_field :photo_url %>
<br>

<%= f.submit "Create" %>

<% end %>
